<style>
    h1{
        text-align: center;
    }
</style>
<h1>Update Student</h1><br>
<% result.forEach(function(user){ %>

    <form method="post">
        <table border="" align="center">
            <tr>
                <td>Name:</td>
                <td><input type="text" name="name" value="<%=user.name%>"></td>
            </tr>
    
            <tr>
                <td>Std:</td>
                <td><input type="number" name="std" value="<%=user.std%>"></td>
            </tr>
            <tr>
                <td>Div:</td>
                <td><input type="text" name="div" value="<%=user.division%>"></td>
            </tr>
            <tr>
                <td>Roll No:</td>
                <td><input type="number" name="roll_no" value="<%=user.roll_no%>"></td>
            </tr>
            <tr align="center">
                <td colspan="2"><input type="submit" value="Update" name="save"></td>
            </tr>
        </table>
    </form>
    
    <% }); %>