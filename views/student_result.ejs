</html>
<br><h1>Add Result</h1><br>
<form method="post">
    <table align="center" border="1">

        <% result.forEach(function(user){ %>
            <tr>
                <td>Name</td>
                <td><input type="text" name="name" value="<%= user.name%>"></td>
            </tr>
            <tr>
                <td>Std</td>
                <td><input type="text" name="std" value="<%= user.std%>"></td>
            </tr>
            <tr>
                <td>Division</td>
                <td><input type="text" name="div" value="<%= user.division%>"></td>
            </tr>
            <tr>
                <td>Roll No.</td>
                <td><input type="text" name="roll_no" value="<%= user.roll_no%>"></td>
            </tr>
        <% }); %>
        <tr>
            <td>Subject-1</td>
            <td><input type="number" name="sub1"></td>
        </tr>
        <tr>
            <td>Subject-2</td>
            <td><input type="number" name="sub2"></td>
        </tr>
        <tr>
            <td>Subject-3</td>
            <td><input type="number" name="sub3"></td>
        </tr>
        <tr>
            <td>Subject-4</td>
            <td><input type="number" name="sub4"></td>
        </tr>
        <tr>
            <td>Subject-5</td>
            <td><input type="number" name="sub5"></td>
        </tr>
        <tr>
            <td></td>
            <td align="center"><input type="submit" value="Save"></td>
        </tr>
    </table>
</form>